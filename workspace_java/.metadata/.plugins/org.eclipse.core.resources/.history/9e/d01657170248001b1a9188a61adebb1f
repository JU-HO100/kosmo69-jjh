<update id="updateStep" parameterType="map">
   update board_master
      set bm_step = bm_step + 1
      where bm_group =35
      and bm_step >1
</update>   

<update id="memberUpdate" parameterType="map">
   Update board_master
      set bm_title = #{bm_title}
      ,bm_contente = #{bm_contente}
      ,bm_writer = #{bm_writer}
      ,bm_pw = #{bm_pw}
      ,bm_email = #{bm_emaile}
      ,bm_date = TO_CHAR(sysdate, 'YYYY-MM-DD')
      where bm_no = #{bm_no}
   </update>
   
<update id="memberUpdate" parameterType="map">
   Update board_sub
      set bm_file = #{bm_file}
      ,bm_size = #{bm_size}
      where bm_no =  #{bm_no}
   </update>
   
<delete id="memberDelete" parameterType="map">
   Delete board_master
      where bm_no =  #{bm_no}
